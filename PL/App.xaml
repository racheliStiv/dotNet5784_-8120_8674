<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PL"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:EngineerExperienceCollection x:Key="EngineerExperienceCollectionKey"/>
        <local:ConvertIdToContent x:Key="ConvertIdToContentKey"/>
        <local:IsEnableConverter x:Key="IsEnableConverterKey"/>
        <local:IsVisibleConverter x:Key="IsVisibleConverterKey"/>
        <local:TaskStatusCollection x:Key="TaskStatusCollectionKey"/>
        <local:IsCurrentTask x:Key="IsCurrentTaskKey"/>
        <local:ConvertStatusToColor x:Key="ConvertStatusToColorKey"/>
        <local:ConvertStatusToForground x:Key="ConvertStatusToForgroundKey"/>
    </Application.Resources>
</Application>

